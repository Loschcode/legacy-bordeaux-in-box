<div class="tablechild">
  <div class="row">
    <div class="grid-6">
      <h3 class="tablechild__title">A propos</h3>
      <strong>Role:</strong> <%- @role_format %><br/>
      <strong>Total Pay√©:</strong> <%- _.euro(@turnover) %><br/>

      <% if @address.length > 0: %>
        <strong>Adresse:</strong> <%- @address %>, <%- @city %> (<%- @zip %>)
      <% else: %>
        N/A
      <% end %>
    </div>
    <div class="grid-6">
      <h3 class="tablechild__title">Abonnements</h3>
      <% if @profiles.length > 0: %>
        <% for profile in @profiles: %>
          <a class="tablechild__link" href="<%- _.slash(@edit_profile) + profile.id %>">Abonnement #<%= profile.id %> (<%- _.profileStatus(profile.status) %>)</a><br/>
        <% end %>
      <% else: %>
        Aucun abonnement
      <% end %>
    </div>
  </div>
</div>